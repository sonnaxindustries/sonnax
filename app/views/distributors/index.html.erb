<h1>Distributors</h1>
<% if @distributors.any? %>
  <table cellpadding="0" cellspacing="0" summary="Listing of Distributors">
    <thead>
      <tr>
        <th>Name</th>
        <th>City</th>
        <th>State</th>
        <th>Country</th>
        <th>Phone Number</th>
        <th>Website URL</th>
      </tr>
    </thead>
    <tbody>
      <% @distributors.each do |distributor| %>
      <tr>
        <td><%= link_to distributor.name, distributor_path(distributor.url_friendly) %></td>
        <td><%= distributor.city %></td>
        <td><%= distributor.state %></td>
        <td><%= distributor.country %></td>
        <td><%= distributor.phone_number %></td>
        <td><%= link_to distributor.website_url, distributor.website_url.to_s, :rel => 'external' %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
<% else %>
<p>There are no distributors.</p>
<% end %>