<h1>Edit: <%= @publication_subject.title %></h1>
<ul>
  <li><%= link_to 'Delete', admin_publication_subject_path(@publication_subject), :class => 'destroy-html', :method => :delete, :confirm => 'Are you sure you want to delete this Category?' %></li>
</ul>
<% form_for :publication_subject, @publication_subject, :url => admin_publication_subject_path(@publication_subject), :html => { :method => :put } do |f| %>
  <%= render :partial => 'form', :locals => { :f => f } %>
  
  <div class="submit">
    <%= f.submit 'Save' %> or <%= link_to 'Cancel', admin_publication_subjects_path, :class => 'cancel' %>
  </div>
<% end %>

