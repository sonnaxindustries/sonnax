<h1>Add a new Make</h1>
<% form_for :make, @make, :url => admin_makes_path, :html => { :method => :post } do |f| %>
  <%= render :partial => 'form', :locals => { :f => f } %>
  
  <div class="submit">
    <%= f.submit 'Save' %> or <%= link_to 'Cancel', admin_makes_path, :class => 'cancel' %>
  </div>
<% end %>