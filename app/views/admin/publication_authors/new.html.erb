<h1>Add a new Author</h1>
<% form_for :publication_author, @publication_author, :url => admin_publication_authors_path do |f| %>
  <%= render :partial => 'form', :locals => { :f => f } %>
  
  <div class="submit">
    <%= f.submit 'Save' %> or <%= link_to 'Cancel', admin_publication_authors_path, :class => 'cancel' %>
  </div>
<% end %>

