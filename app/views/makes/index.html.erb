<h1>Makes</h1>
<% if @makes.any? %>
  <table cellpadding="0" cellspacing="0" summary="Listing of Makes">
    <thead>
      <tr>
        <th>Name</th>
      </tr>
    </thead>
    <tbody>
      <% @makes.each do |make| %>
      <tr>
        <td><%= link_to make.name, make_path(make) %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
<% end %>