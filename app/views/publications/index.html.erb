<h2>Publications</h2>
<% if @publication_categories.any? %>
  <% @publication_categories.each do |category| %>
    <h3><%= category.name %></h3>
    <% if category.publications? %>
      <% category.publications.each do |publication| %>
        <p><%= link_to publication.title, '#' %></p>
      <% end %>
    <% end %>
  <% end -%>
<% else %>
<p>There are no Publications at this time.</p>
<% end %>